{
  "compilerOptions": {
    "_comment_for_common": [
      "react",
      "express"
    ],
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "moduleResolution": "Node",
    "importsNotUsedAsValues": "error",
  
    "_comment_for_react": ["react"],
    "jsx": "react",
    "target": "ES5",
    "downlevelIteration": true
  }
}
