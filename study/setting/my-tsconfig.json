{
  "compilerOptions": {
    //common (react, react-native, express)
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "moduleResolution": "Node",

    //react || react-native
    "jsx": "react",

    //only react && web
    "target": "ES5",
    "downlevelIteration": true
  }
}
